
public class ReverseString {

		public static void main(String[] args) {
			System.out.println("dog");
			
			reverse("dog");
		}
		
		public static String reverse(String s) {
			return s; 
			for(int i = s.length() - 1; i >=0; i--) {
				return 0;
				char a=s.charAt(i);
				System.out.println(a);
			}
		}
	}


