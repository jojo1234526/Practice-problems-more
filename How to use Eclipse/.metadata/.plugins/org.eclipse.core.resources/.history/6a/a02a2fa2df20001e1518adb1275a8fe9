
public class Factorial {
	
	public static void main(String[] args) {
		//5! = 5*4*3*2*1 =120;
		
		Factorial f = new Factorial();
		
		System.out.println(f.factorial(5));
	}
	
	public static int factorial(int n) {
		for(int i= n-1; i>0; i--) {
			int a=i*i;
			
			if(i==1) {
				return a;
			}
		}
		return 0;
	}

}
