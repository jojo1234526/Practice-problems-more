
public class Factorial {
	
	public static void main(String[] args) {
		//5! = 5*4*3*2*1 =120;
		
		Factorial f = new Factorial();
		
		System.out.println(factorial(5));
	}
	
	public static int factorial(int n) {
		int b = 0;
		for(int i= n; i>0; i--) {
			int a=i*i;
			System.out.println(b);
			b+=a;
			if(i==1) {
				return a;
			}
		}
		return 0;
	}

}
